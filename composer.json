{
    "name": "code4/settings",
    "description": "Settings package with user / global inheritance",
    "homepage": "https://github.com/code4interactive/settings",
    "license": "MIT",
    "authors": [
        {
            "name": "Artur Bartczak",
            "email": "artur.bartczak@code4.pl"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/support" : "~5.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Code4\\Settings\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Code4\\Settings\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
